{
  "manifest_version": 3,
  "name": "chromeZoom",
  "version": "1",
  "description": "CTRL + Mouse wheel zoom",
  "homepage_url": "https://github.com/benjaminkwhite/chromeZoom",
  "minimum_chrome_version": "46",
  "icons": {
    "128": "images/icon128.png",
    "16": "images/icon16.png",
    "48": "images/icon48.png"
  },
  "content_scripts": [ {
    "js": [ "js/content.js" ],
    "matches": [ "<all_urls>"],
    "run_at": "document_end"
  } ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "images/icon19.png"
  }
}
